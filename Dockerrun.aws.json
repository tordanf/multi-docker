{
		"AWSEBDockerrunVersion": 2,
		"containerDefinitions": [
				{
						"name": "client",
						"image": "tordanf/multi-client",
						"hostname": "client",
						"essential": false,
						"memory": 128
				},
				{
						"name": "server",
						"image": "tordanf/multi-server",
						"hostname": "api",
						"essential": false,
						"memory": 128
				},
				{
						"name": "worker",
						"image": "tordanf/multi-worker",
						"hostname": "worker",
						"essential": false,
						"memory": 128
				},
				{
						"name": "nginx",
						"image": "tordanf/multi-nginx",
						"hostname": "nginx",
						"essential": true,
						"postMappings": [
								{
										"hostPort":80,
										"containerPort":80
								}
						],
						"links": ["client", "server"],
						"memory": 128
				}
		]
}
